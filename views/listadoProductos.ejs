<h1>listado de productos</h1>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Blockbuster</title>
    <link rel="stylesheet" href="css/styles-home.css">
    <link rel="stylesheet" href="/css/administrarProductos.css">

    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500&display=swap" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.0/css/all.min.css" rel="stylesheet" />
    <link rel="icon" href="/images/logo-mercado-liebre.svg">
    
</head>
<body>
    
    <%- include('partials/header') %>  

    <% listaPeliculas.forEach(element => { %>

        

        <article class="card">
            <img class="img-pelicula" src="<%= element.imagen %> ">
            <h2 class="tituloPeli"><%= element.titulo %></h2>
            <h3 class="texto"><%= element.descripcion %></h3>
            <h3 class="texto"><%= element.precio %></h3>
            <a href="/productDetail?<%= element.id %> " class="botonFormBusc">Ver</a>
        </article>
    <% }); %> 


    <%- include('partials/footer') %> 

</body>
</html>